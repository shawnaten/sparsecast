<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="MaterialTextView">
        <attr name="family" format="enum" >
            <enum name="normal" value="0"/>
            <enum name="condensed" value="1"/>
        </attr>
        <attr name="style" format="enum">
            <enum name="thin" value="0"/>
            <enum name="light" value="1"/>
            <enum name="regular" value="2"/>
            <enum name="medium" value="3"/>
            <enum name="bold" value="4"/>
            <enum name="black" value="5"/>
        </attr>
        <attr name="italicized" format="boolean"/>
    </declare-styleable>
</resources>
